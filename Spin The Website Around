javascript:(function(){    var angle = 0;    var spin = setInterval(function(){        angle+=5;        document.body.style.transform = "rotate("+angle+"deg)";    }, 30);    var stopButton = document.createElement("button");    stopButton.innerHTML = "Stop spinning";    document.body.appendChild(stopButton);    stopButton.style.position = "absolute";    stopButton.style.top = "0px";    stopButton.style.right = "0px";    stopButton.onclick = function() {        clearInterval(spin);        stopButton.style.display = "none";    };})();
